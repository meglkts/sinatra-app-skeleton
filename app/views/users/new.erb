<h1>Sign up to begin managing your TO DO lists</h1>

    <form class="singup_form" action="/users" method="POST">
      <label>Display Name</label><br>
      <input type="text" name="user[name]" placeholder="J. Doe"><br>
      <label>Email (required, but never shown)</label><br>
      <input type="email" name="user[email]" placeholder="you@example.org"><br>
      <label>Password</label><br>
      <input type="password" name="user[password]" placeholder="********"><br>
      <!-- <label>Confirm password</label><br>
      <input type="text" name="user[confirm_password]" placeholder="********"><br> -->
      <input type="submit" value="Sign up">
    </form>
